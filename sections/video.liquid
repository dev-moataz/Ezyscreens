{% schema %}
{
  "name": "hero (Image/video)",
  "tag": "section",
  "class": "section transparent_header",
  "disabled_on": {
    "groups": ["header", "footer"]
  },
  "settings": [
    {
      "type": "header",
      "content": "content",
    },
    {
      "type": "text",
      "id": "subheading",
      "label": "Subheading",
      "default": "OVER 13 YEARS OF EXPERIENCE"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Main Heading",
      "default": "Premium Quality Retractable Screens"
    },
    {
      "type": "text",
      "id": "description",
      "label": "Description",
      "default": "Perfect blend of functionality and style in retractable fly screens and blackout screens."
    },
    {
      "type": "text",
      "id": "button_text",
      "label": "Button Text",
      "default": "OUR PRODUCTS"
    },
    {
      "type": "url",
      "id": "button_link",
      "label": "Button Link"
    },
    
    {
      "type": "checkbox",
      "id": "show_phone",
      "label": "Show call us link",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "show_email",
      "label": "Show email us link",
      "default": false
    },
    {
      "type": "header",
      "content": "video",
    },
    {
      "type": "checkbox",
      "id": "enable_video_looping",
      "label": "t:sections.video.settings.enable_video_looping.label",
      "default": false
