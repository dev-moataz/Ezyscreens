{{ 'video-section.css' | asset_url | stylesheet_tag }}
{{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  {% if section.settings.add_overlay == true %}
    .section-{{ section.id }}-padding.video-section:before {
    content: "";
    left: 0;
    width: 100%;
    height: 100%;
    top: 0;
    position: absolute;
    background: rgba(0,0,0,0.{{-section.settings.overlay_opacity-}});
    z-index: 2;
}
  {% endif %}
{%- endstyle -%}

{%- liquid
  assign video_id = section.settings.video.id 
  assign video_alt = section.settings.video.alt | default: section.settings.video_description
  assign alt = 'sections.video.load_video' | t: description: video_alt | escape
  assign poster = section.settings.video.preview_image | default: section.settings.cover_image

  if section.settings.video != null
    assign ratio_diff = section.settings.video.aspect_ratio | minus: poster.aspect_ratio | abs
    if ratio_diff < 0.01 and ratio_diff > 0
      assign fix_ratio = true
    endif
  endif
-%}
<style>
.video-section{
  position: relative;
}
.section-{{ section.id }}-padding .video-container-box{
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 5;
  margin-block: 5rem;
  width: 100%;
}
.section-{{ section.id }}-padding .video-container-box.center-content{
  bottom: 40%;
  left: 50%;
  transform: translate(-50%, 50%);
  text-align: center;
}
.section-{{ section.id }}-padding .video-container-box.center-content .description{
  max-width: 800px;
  margin-inline: auto;
}
.section-{{ section.id }}-padding .video-container-box.center-content .hero-cta{
  justify-content: center;
}
.section-{{ section.id }}-padding .video-container-box.center-content .title-wrapper{
  max-width: initial;
}
.section-{{ section.id }}-padding .video-container-box .title-wrapper{
  max-width: 750px;
}
.template-index  .video-container-box .description{
  
    max-width: 500px;
    line-height: 1.56;
}
{% if section.settings.video != blank %}
  .section-{{ section.id }}-padding .video-section__media{
  --ratio-percent:45% !important;
}
/* small desktops */
@media screen and (max-width: 1199px) {
.section-{{ section.id }}-padding .video-section__media{
  --ratio-percent:57% !important;
}
}
/* tablet */
@media screen and (max-width: 989px) {
.section-{{ section.id }}-padding .video-section__media{
  --ratio-percent:80% !important;
}
}
/* mobile */
@media screen and (max-width: 749px) {
.section-{{ section.id }}-padding .video-section__media{
  --ratio-percent:80vh !important;
}
}
@media screen and (max-width: 400px) {
.section-{{ section.id }}-padding .video-section__media{
  --ratio-percent:92vh !important;
}
}
{% else %}
.section-{{ section.id }}-padding  .deferred-media__poster{
  cursor: initial;
}
.section-{{ section.id }}-padding  .div__poster-button{

    display: none !important;
}
/* mobile */
@media screen and (max-width: 749px) {
.section-{{ section.id }}-padding .video-section__media{
  --ratio-percent:70vh !important;
}
}

{% endif %}

/* mobile */
@media screen and (max-width: 749px) {

.section-{{ section.id }}-padding .video-container-box{
        margin-block: 1rem;
}

}

</style>
{%- capture sizes -%}
  {% if section.settings.full_width -%}
    100vw
  {%- else -%}
    (min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 }}px, (min-width: 750px)
    calc(100vw - 10rem), 100vw
  {%- endif %}
{%- endcapture -%}

